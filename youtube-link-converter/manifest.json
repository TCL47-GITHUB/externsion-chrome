{
    "manifest_version": 3,
    "name": "YouTube Link Converter",
    "version": "1.0",
    "description": "Convert YouTube links into embeddable iframe codes.",
    "icons": {
      "16": "assets/icon.png",
      "48": "assets/icon.png",
      "128": "assets/icon.png"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": "assets/icon.png"
    },
    "permissions": ["clipboardWrite"]
  }
  